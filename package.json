{
  "name": "express-pig",
  "version": "0.5.6",
  "main": "index.js",
  "repository": "git@github.com:DmytroMysak/koa-pig.git",
  "author": "Dmytro Mysak",
  "license": "MIT",
  "scripts": {
    "build": "gulp",
    "start-dev": "gulp run",
    "start": "gulp && better-npm-run start-prod",
    "start-dev-pm2": "gulp && better-npm-run start-dev"
  },
  "betterScripts": {
    "start-prod": {
      "command": "pm2 start dist/index.js --name bot-prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "pm2 start dist/index.js --name bot-dev",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "aws-sdk": "^2.262.1",
    "axios": "^0.18.0",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-winston": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^3.12.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "ngrok": "^3.0.1",
    "p-queue": "^3.0.0",
    "pg": "^7.4.3",
    "play-sound": "^1.1.2",
    "pm2": "^3.0.4",
    "sequelize": "^4.38.0",
    "telegraf": "^3.21.2",
    "winston": "^3.1.0",
    "ytdl-core": "^0.25.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.12.0",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-sourcemaps": "^2.6.4"
  }
}
