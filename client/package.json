{
  "name": "pig-client",
  "version": "0.7.0",
  "main": "index.js",
  "repository": "git@github.com:DmytroMysak/koa-pig.git",
  "author": "Dmytro Mysak",
  "license": "MIT",
  "scripts": {
    "build": "gulp",
    "start-dev": "gulp run",
    "start": "gulp && better-npm-run start-prod",
    "start-dev-pm2": "gulp && better-npm-run start-dev"
  },
  "betterScripts": {
    "start-prod": {
      "command": "pm2 start dist/index.js --name bot-prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "pm2 start dist/index.js --name bot-dev",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "^0.1.0",
    "dotenv": "^6.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "p-queue": "^3.0.0",
    "pino": "^5.10.6",
    "play-sound": "^1.1.2",
    "pm2": "^3.0.4",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "del": "^4.0.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "pino-pretty": "^2.5.0"
  }
}
