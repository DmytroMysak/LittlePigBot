{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "max-len": ["error", { "code": 130 }],
        "object-curly-newline": ["error", {
            "minProperties": 6,
            "consistent": true
        }]
    }
}
